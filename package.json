{
  "name": "discord-music-bot",
  "version": "0.5.0",
  "description": "The music bot for discord",
  "main": "src/main.js",
  "scripts": {
    "start": "ts-node-dev -r dotenv/config -r tsconfig-paths/register src/main.ts",
    "build": "tsc",
    "lint": "eslint . src/**/*.{ts,js} --fix",
    "format": "prettier --config .prettierrc src/**/*.{ts,js} --write",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "url": "https://github.com/Krysik/discord-music-bot"
  },
  "author": "Marcin Krysi≈Ñski <marcin_krysinski@outlook.com>",
  "license": "ISC",
  "dependencies": {
    "discord-api-types": "^0.25.2",
    "discord-player": "^5.3.1",
    "discord.js": "^14.4.0",
    "dotenv": "^16.0.0",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "tsconfig-paths": "^4.0.0",
    "youtube-sr": "^4.1.15"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
