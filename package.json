    "@discordjs/rest": "0.1.0-canary.0",
    "discord-api-types": "^0.25.2",
    "discord-player": "^5.2.2",
    "discord.js": "^13.3.1",
    "dotenv": "^16.0.0",
    "ffmpeg-static": "^4.4.0",
    "pino": "^7.6.1",
    "pino-pretty": "^7.3.0",
    "tsconfig-paths": "^4.0.0",
